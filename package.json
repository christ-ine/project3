{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm start  --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "local": "concurrently \"node ./bin/www\" \"cd client && npm start\"",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect": "^3.7.0",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react-pose": "^4.0.8",
    "react-spring": "^8.0.20",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
